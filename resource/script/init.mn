
class Base
{
	var id = 1;
	var name = "i'm base";
	constructor()
	{
		::print("base ", name);
	};
	func foo()
	{
		::print(this);
		::print("Base ", name);
	};
};

class Test : Base
{
	var name = "i'm test";
	constructor()
	{
		base.constructor();
		::print("test ", name);
	};
	func foo()
	{
		::print(name);
		::print(base.foo());
	};
};

var t0 = 
{
	name : "i'm table"
};

setmeta(t0, Base);
t0.foo();
