
var GameObject = 
{
	array:[],
	type : "GameObject",
	new  : func()
	{
		var inst = 
		{
			name       : null,
			x          : 0.0,
			y          : 0.0,
			scaleX     : 1.0,
			scaleY     : 1.0,
			texture    : null,
			components : [],
		};
		return ::setmeta(inst, this);
	},
	update : func()
	{
		var i = 0;
		var count = components.count();
		while (i < count)
		{
			var comp = components[i];
			comp.update();
			comp.updateCoroutine();
		}
	},
};
